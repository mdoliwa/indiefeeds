<%= form_with(model: @feed, html: { class: 'max-w-xl' }) do |form| %>

  <div class='flex flex-col mb-4'>
    <%= form.label :url %>
    <%= form.text_field :url, class: "border border-gray-300 px-2 py-1" %>
  </div>

  <div class='mb-2'>
    <%= form.submit "Add new feed", class: "bg-blue-100 px-3 py-1 md:px-8 border border-blue-900 hover:bg-blue-200 w-full md:w-max"%>
  </div>
<% end %>
